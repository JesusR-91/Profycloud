<h2>Welcome to {{oneClass.name}} {{oneClass.subName}}:</h2>
<br>
<div class="class">
  <div class="oneClass card-group">
    <div class="container card">
      <span>Alumnos:</span>
    <br>
      <div class="alumns">
        {{#each oneClass.alumns}}
          <a href="/alumn/{{_id}}/details">{{firstName}} {{lastName}}</a>
          <br>
        {{/each}}
      </div>
    </div>
    <div class="container card">
    <span>Profesores:</span>
    <br>
      {{#each professorsClass}}
        <a href="/admin/{{_id}}">{{firstName}} {{lastName}}</a>
        <br>
      {{/each}}
    </div>

    <br>
  </div>
  <br>
      {{#if isTutor}}
        <button class="btn btn-secondary "><a href="/alumn/create"> Add a new alumn</a></button>
      {{/if}}

</div>